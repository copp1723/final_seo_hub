{
  "name": "rylie-seo-hub",
  "version": "2.0.0",
  "description": "AI-Powered SEO Request Management Platform",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && npm run type-check && next build",
    "start": "next start -p ${PORT:-3001}",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:redis": "jest lib/__tests__/csrf-redis.test.ts lib/__tests__/rate-limit-redis.test.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:studio": "prisma studio",
    "postinstall": "prisma generate",
    "clean": "rm -rf .next node_modules",
    "clean:install": "npm run clean && npm install",
    "build:production": "prisma generate && prisma migrate deploy && npm run type-check && next build"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.2",
