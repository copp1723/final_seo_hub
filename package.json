{
  "name": "gseo-hub",
  "version": "2.0.0",
  "description": "AI-Powered SEO Request Management Platform",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npx prisma generate && prisma db execute --schema=prisma/schema.prisma --file=scripts/fix-package-assignments.sql || true && next build",
    "start": "next start -p ${PORT:-3000}",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:redis": "jest lib/__tests__/csrf-redis.test.ts lib/__tests__/rate-limit-redis.test.ts",
    "validate:mappings": "npx tsx scripts/validate-dealership-mappings.ts",
    "validate:mappings:template": "npx tsx scripts/validate-dealership-mappings.ts --template",
    "test:ga4": "jest __tests__/ga4-property-selection.test.ts",
    "test:ga4:watch": "jest __tests__/ga4-property-selection.test.ts --watch",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:fix": "node scripts/pre-deploy-fix.js",
    "db:sync": "./scripts/bypass-migrations.sh",
    "db:migrate:dev": "prisma migrate dev",
    "db:studio": "prisma studio",
    "postinstall": "prisma generate",
    "prestart": "prisma db execute --schema=prisma/schema.prisma --file=scripts/fix-package-assignments.sql || true",
    "clean:install": "npm run clean && npm install",
    "build:production": "prisma generate && prisma migrate deploy && npm run db:assign-packages && next build",
    "db:setup": "prisma generate && prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts",
    "db:setup-admins": "prisma db execute --schema=prisma/schema.prisma --file=scripts/setup-super-admins.sql",
    "db:assign-packages": "prisma db execute --schema=prisma/schema.prisma --file=scripts/fix-package-assignments.sql",
    "db:reset-with-admins": "prisma migrate reset --force && npm run db:setup-admins",
    "db:seed-sql": "prisma db execute --file=scripts/seed-data.sql",
    "test:integration": "node scripts/integration-test.js",
    "test:integration:verbose": "DEBUG=1 node scripts/integration-test.js"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.2",
    "@hookform/resolvers": "^3.10.0",
    "@prisma/client": "^6.12.0",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.2",
    "@radix-ui/react-toast": "^1.2.14",
    "@sentry/nextjs": "^10.0.0",
    "autoprefixer": "^10.4.21",
    "browserslist": "^4.25.1",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "form-data": "^4.0.3",
    "glob": "^11.0.3",
    "google-auth-library": "^10.1.0",
    "googleapis": "^150.0.1",
    "ioredis": "^5.6.1",
    "jose": "^6.0.12",
    "lucide-react": "^0.522.0",
    "mailgun.js": "^12.0.3",
    "next": "^14.2.5",
    "next-auth": "5.0.0-beta.25",
    "node-fetch": "^3.3.2",
    "node-releases": "^2.0.19",
    "openai": "^5.11.0",
    "papaparse": "^5.5.3",
    "pg": "^8.16.3",
    "postcss": "^8.5.1",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.55.0",
    "recharts": "^3.1.0",
    "sonner": "^2.0.5",
    "sqlite3": "^5.1.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.5.0",
    "uuid": "^11.1.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.9",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/recharts": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "audit-ci": "^7.1.0",
    "depcheck": "^1.4.7",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5",
    "jest": "^30.0.3",
    "knip": "^5.61.3",
    "prettier": "^3.5.3",
    "prisma": "^6.12.0",
    "redis-memory-server": "^0.12.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "ts-prune": "^0.10.3",
    "tsx": "^4.20.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
